<h1>list of manufacturers</h1>
<table>
  <thead>
    <tr>
      <th>name</th>
      <th>country</th>
    </tr>
  </thead>
  <tbody>
    <% @manufacturers.each do |manufacturer| %>
      <tr>
        <td><%= manufacturer.name %></td>
        <td><%= manufacturer.country %></td>
      </tr>
    <% end %>
  </tbody>
</table>
